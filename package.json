{
  "name": "stachine",
  "version": "1.1.1",
  "description": "A TypeScript state machine with events and effects",
  "keywords": [
    "state-machine",
    "typescript"
  ],
  "homepage": "https://github.com/etienne-dldc/stachine#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/stachine/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/etienne-dldc/stachine.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist deno_dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts && denoify",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "test:watch:cov": "jest --watch --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "suub": "^3.3.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.37",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "denoify": "^1.4.10",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "np": "^7.6.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.3.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
