{
  "name": "stachine",
  "version": "1.0.10",
  "description": "A TypeScript state machine with events and effects",
  "keywords": [
    "state-machine",
    "typescript"
  ],
  "homepage": "https://github.com/etienne-dldc/stachine#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/stachine/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/etienne-dldc/stachine.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist deno_dist && tsup --format cjs,esm src/mod.ts --dts src/mod.ts && denoify",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "test:watch:cov": "jest --watch --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "suub": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "denoify": "^0.11.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.3",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "tslib": "^2.4.0",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
